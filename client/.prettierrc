{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "bracketSameLine": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "proseWrap": "never",
  "quoteProps": "as-needed"
}
